{
  "name": "react-library",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=config/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint './src/app/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=config/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest --watch --coverage --config=config/jest.json"
  },
  "private": true,
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "@types/node": "^10.5.1",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.2.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "react": "^16.4.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "bootstrap": "^4.1.1",
    "core-js": "^2.4.1"
  },
  "dependencies": {
    "imagemin-pngquant": "^5.0.1"
  },
  "postcss": {}
}
